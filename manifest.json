{  
  "manifest_version": 2,
  "name": "Color Annihlator",
  "description": "This extension gradually takes color away from the current web page",
  "version": "1.0",

  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["NoColor.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab"
  ],
  "icons": {"16": "skully16.png",
            "48": "skully48.png",
            "128": "skully128.png" }
}
